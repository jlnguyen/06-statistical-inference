rev <- 39e6
rev.g <- 0.065
cost <- 10e6
cost.g <- 0.03
admin <- 0.8e6
tax <- 0.35
capex <- 225e6
r <- 0.06
n <- 18
deprec <- capex / n
termVal <- 75e6
# Periodic cash flows (in period $ values)
cVec <- rep(0,n)
for (i in 1:n) {
# Operating profit
op <- FV(rev, rev.g, i-1) - FV(cost, cost.g, i-1) - admin - deprec
# Net operating profit after tax
nopat <- (1 - tax) * op
# Cash flow
cVec[i] <- nopat + deprec
}
# NPV
NPV(capex, cVec, r) + PV(termVal, r, n)
